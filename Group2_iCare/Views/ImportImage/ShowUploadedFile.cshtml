@{
    ViewBag.Title = "Show Uploaded File";
}

<h2>Show Uploaded File</h2>

<div>
    <p><strong>Patient Record ID:</strong> @ViewBag.PatientRecordID</p>
    <p><strong>File Path:</strong> @ViewBag.FilePath</p>
</div>

<div>
    <iframe src="@ViewBag.FilePath" width="100%" height="600px"></iframe>
</div>

<div>
    @Html.ActionLink("Import Another Image", "Index", new { PatientRecordID = ViewBag.PatientRecordID })
</div>
